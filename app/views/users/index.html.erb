<h3>User Administration</h3>
<div class="span8">
<table class="table table-condensed">
	  <thead>
      <tr>
          <th>Username</th>
          <th>Email</th>
          <th>Registered</th>
          
          <th>Provider</th>
          <th></th>                                    
      </tr>
  </thead>   
  <tbody>
  <% @users.each do |user| %>
    <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.created_at.to_date %></td>
        
        <td><%= user.provider %></td>
        
    </tr>
  <% end %>
  </tbody>
</table>
</div>